import random

avengers=["Hulk","Iron man","Captain America","Falcon","Ant man","Black widow","Thor","Dr strange"]
l=len(avengers)
k=random.randint(0,l-1)

print(avengers[k])
s=""
for i in avengers[k]:
    if(i==" " or i=="a" or i=="e" or i=="i" or i=="o" or i=="u"):
        print(i)
        s+=" "
    else:
       s+="_ "
print(s)
